{
  "name": "treehouselabs/keystone-bundle",
  "type": "symfony-bundle",
  "description": "A Symfony2 implementation of the OpenStack Identity Service API: Keystone",
  "keywords": ["openstack", "identity", "keystone", "authentication", "token"],
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Kruithof",
      "email": "peter@treehouse.nl"
    },
    {
      "name": "Jeroen Fiege",
      "email": "jeroen@treehouse.nl"
    }
  ],
  "require": {
    "php": ">=5.6",
    "symfony/symfony": "2.6.*",
    "doctrine/orm": "^2.2.3",
    "doctrine/doctrine-bundle": "^1.2",
    "symfony/monolog-bundle": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "TreeHouse\\KeystoneBundle\\": "src/TreeHouse/KeystoneBundle/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TreeHouse\\KeystoneBundle\\Test\\": "tests/TreeHouse/KeystoneBundle/Test/",
      "TreeHouse\\KeystoneBundle\\Tests\\": "tests/TreeHouse/KeystoneBundle/Tests/",
      "TreeHouse\\KeystoneIntegrationBundle\\": "tests/Functional/src/TreeHouse/KeystoneIntegrationBundle/"
    }
  }
}
